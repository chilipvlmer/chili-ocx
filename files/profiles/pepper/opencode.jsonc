{
	"agent": {
		"scoville-orchestrator": {
			"tools": {
				"edit": false,
				"write": false,
				"bash": false,
				"question": true
			}
		},
		"seed-prd-rfc": {
			"tools": {
				"bash": false,
				"question": true
			},
			"permission": {
				"edit": {
					".pepper/**": "allow",
					"*": "deny"
				},
				"write": {
					".pepper/**": "allow",
					"*": "deny"
				}
			}
		},
		"sprout-execution-planner": {
			"tools": {
				"bash": false,
				"question": true
			},
			"permission": {
				"edit": {
					".pepper/**": "allow",
					"*": "deny"
				},
				"write": {
					".pepper/**": "allow",
					"*": "deny"
				}
			}
		},
		"jalapeno-coder": {
			"tools": {
				"question": true
			},
			"permission": {
				"shadcn_*": "allow"
			}
		},
		"habanero-reviewer": {
			"tools": {
				"edit": false,
				"write": false
			},
			"permission": {
				"bash": {
					"git diff*": "allow",
					"git log*": "allow",
					"git status": "allow",
					"*": "deny"
				}
			}
		},
		"ghost-explorer": {
			"tools": {
				"edit": false,
				"write": false
			},
			"permission": {
				"bash": {
					"find*": "allow",
					"grep*": "allow",
					"rg*": "allow",
					"cat*": "allow",
					"head*": "allow",
					"tail*": "allow",
					"wc*": "allow",
					"ls*": "allow",
					"*": "deny"
				},
				"context7_*": "allow",
				"exa_*": "allow",
				"gh_grep_*": "allow"
			}
		},
		"chipotle-scribe": {
			"tools": {
				"question": true
			},
			"permission": {
				"context7_*": "allow"
			}
		}
	},
	"permission": {
		"shadcn_*": "deny",
		"context7_*": "deny",
		"exa_*": "deny",
		"gh_grep_*": "deny",
		"playwright_*": "deny"
	},
	"mcp": {
		"shadcn": {
			"type": "remote",
			"url": "https://mcp.shadcn.com",
			"enabled": true
		},
		"context7": {
			"type": "remote",
			"url": "https://mcp.context7.com/mcp",
			"enabled": true
		},
		"exa": {
			"type": "remote",
			"url": "https://mcp.exa.ai/mcp",
			"enabled": true
		},
		"gh_grep": {
			"type": "remote",
			"url": "https://mcp.grep.app",
			"enabled": true
		},
		"playwright": {
			"type": "remote",
			"url": "https://mcp.microsoft.com/playwright-mcp",
			"enabled": true
		}
	}
}
